@{
    Layout = null;
    ExpediaHotel hotel = ViewBag.ExpediaHotel;
}
<?xml version="1.0" encoding="UTF-8" ?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Placemark>
    <name>@hotel.BaseItem.Name</name>
    <Point>
      <coordinates>@hotel.BaseItem.Longitude, @hotel.BaseItem.Latitude</coordinates>
    </Point>
  </Placemark>
</kml>
